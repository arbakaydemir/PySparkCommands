#Import the pySpark
import pyspark

#Import necessary libraries
from pyspark.sql import SparkSession
from pyspark.sql.functions import *

#Creating a Spark Session
spark = SparkSession.builder.appName("Airport_Traffic").getOrCreate()

#Reading the data from the source
df = spark.read.csv("/home/rbakydemir/PySparkCommands/airport_traffic_2024.csv", header=True)

df.show(5)

# Check schema
df.printSchema()